{
    "manifest_version": 3,
    "name": "Mola Twitter extension",
    "version": "0.1",
    "permissions": [
        "storage",
        "scripting",
        "webNavigation",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {}
}